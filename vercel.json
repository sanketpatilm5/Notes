{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/index.ts" },
    { "src": "/", "dest": "/client/dist/public/index.html" },
    { "src": "/(.*)", "dest": "/client/dist/public/$1" },
    { "src": "/(.*)", "dest": "/client/dist/$1" }
  ]
}
