{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/public/$1"
    }
  ],
  "functions": {
    "server/**/*.ts": {
      "runtime": "nodejs18.x"
    },
    "server/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}